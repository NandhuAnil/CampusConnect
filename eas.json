{
  "cli": {
    "version": ">= 10.1.1"
  },
  "build": {
    "production": {
      "channel": "production",
      "android": {
        "buildType": "apk",
        "env": {
          "GOOGLE_SERVICES_JSON": "@google-services.json"
        }
      }
    },
    "profile-build": {
      "channel": "profile",
      "android": {
        "buildType": "apk",
        "env": {
          "GOOGLE_SERVICES_JSON": "@google-services.json"
        }
      }
    },
    "staging": {
      "channel": "staging",
      "android": {
        "buildType": "apk",
        "env": {
          "GOOGLE_SERVICES_JSON": "@google-services.json"
        }
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}
